<?xml version="1.0" encoding="UTF-8" ?>

<node name="/">
  <interface name="org.totem.MozillaPluginInterface">
    <method name="Play"/>
    <method name="Pause"/>
    <method name="Stop"/>
    <method name="OpenUrl">
      <arg type="s" name="url" direction="in" />
    </method>
    <method name="GetCurrentTime">
      <arg type="t" name="current_time" direction="out" />
    </method>
    <method name="GetStreamLength">
      <arg type="t" name="length" direction="out" />
    </method>
    <method name="GetVolume">
      <arg type="i" name="volume" direction="out" />
    </method>
    <method name="SetVolume">
      <arg type="i" name="volume" direction="in" />
    </method>
    <method name="SeekTime">
      <arg type="t" name="seek_time" direction="in" />
    </method>
    <method name="GetIsPlaying">
      <arg type="b" name="is_playing" direction="out" />
    </method>
    <method name="Close"/>
  </interface>
</node>
